<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:ignore="MissingDefaultResource">

    <EditText
        android:id="@+id/etname03"
        android:layout_width="500dp"
        android:layout_height="62dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:textSize="35dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/etnumber03"
        android:layout_width="58dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:textSize="18dp"
        app:layout_constraintStart_toEndOf="@+id/tvnumber03"
        app:layout_constraintTop_toBottomOf="@+id/etname03"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/btdatepicker03"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/etdateopened03"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <TextView
        android:id="@+id/tvstatus03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="64dp"
        android:text="@string/tv_status04"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <EditText
        android:id="@+id/etdateopened03"
        android:layout_width="180dp"
        android:layout_height="44dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="52dp"
        android:textSize="18dp"
        app:layout_constraintStart_toEndOf="@+id/tvdateopened03"
        app:layout_constraintTop_toBottomOf="@+id/etname03"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/tvnumber03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="@string/tv_number03"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <TextView
        android:id="@+id/tvdateopened03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="64dp"
        android:text="@string/tv_date_opened04"
        android:textSize="18dp"
        app:layout_constraintStart_toEndOf="@+id/spstatus03"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <Spinner
        android:id="@+id/spstatus03"
        android:layout_width="130dp"
        android:layout_height="44dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="52dp"
        android:textSize="18dp"
        app:layout_constraintStart_toEndOf="@+id/tvstatus03"
        app:layout_constraintTop_toBottomOf="@+id/etname03"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/etreview03"
        android:layout_width="557dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="124dp"
        android:inputType="textMultiLine"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etname03"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/iv403"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="172dp"
        android:background="@drawable/border_imageview"
        app:layout_constraintStart_toEndOf="@+id/iv303"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <ImageView
        android:id="@+id/iv103"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="172dp"
        android:background="@drawable/border_imageview"
        app:layout_constraintStart_toEndOf="@+id/tvpics03"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <ImageView
        android:id="@+id/iv303"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="172dp"
        android:background="@drawable/border_imageview"
        app:layout_constraintStart_toEndOf="@+id/iv203"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <ImageView
        android:id="@+id/iv203"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="172dp"
        android:background="@drawable/border_imageview"
        app:layout_constraintStart_toEndOf="@+id/iv103"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <TextView
        android:id="@+id/tvpics03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="172dp"
        android:text="@string/tv_pics04"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <TextView
        android:id="@+id/tvpicsub103"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="156dp"
        android:layout_marginTop="0dp"
        android:text="@string/pic1"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv103" />

    <TextView
        android:id="@+id/tvpicsub203"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:text="@string/pic2"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/tvpicsub103"
        app:layout_constraintTop_toBottomOf="@+id/iv203" />

    <TextView
        android:id="@+id/tvpicsub303"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:text="@string/pic3"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/tvpicsub203"
        app:layout_constraintTop_toBottomOf="@+id/iv303" />

    <TextView
        android:id="@+id/tvpicsub403"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:text="@string/pic4"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/tvpicsub303"
        app:layout_constraintTop_toBottomOf="@+id/iv403" />

    <Button
        android:id="@+id/btdispose03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="328dp"
        android:text="@string/bt_dispose03"
        android:textSize="22dp"
        app:layout_constraintStart_toEndOf="@+id/btupdate03"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <Button
        android:id="@+id/btupdate03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginTop="328dp"
        android:text="@string/bt_update03"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <TextView
        android:id="@+id/tvreview03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="100dp"
        android:text="@string/tv_review04"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <TextView
        android:id="@+id/tvtype03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="16dp"
        android:text="@string/tv_type04"
        android:textSize="18dp"
        app:layout_constraintStart_toEndOf="@+id/etnumber03"
        app:layout_constraintTop_toBottomOf="@+id/etname03" />

    <Spinner
        android:id="@+id/sptype03"
        android:layout_width="100dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:textSize="18dp"
        app:layout_constraintStart_toEndOf="@+id/tvtype03"
        app:layout_constraintTop_toBottomOf="@+id/etname03"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>